<script>
$(document).ready(function(){
    var $container = $('.entry');
    $container.resizevideos();
    $('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '1px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
    });
    $('.post').unnest({
         yourCaption: ".caption",
         wrapName: ".cap",
         newCaptionUsername: false,
         originalPostCaptionUsername: false,
